@Article{Lee_etal99,
author		= {D.K. Lee and L. Itti and C. Koch and J. Braun},
title		= {Attention activates winner-take-all competition among
		  visual filters},
journal		= {Nature Neuroscience},
year		= {1999},
month		= apr,
volume		= {2},
number		= {4},
pages		= {375--381},
doi		= {10.1038/7286},
url		= {10.1038/7286},
abstract	= {Shifting attention away from a visual stimulus reduces,
		  but does not abolish, visual discrimination performance.
		  This residual vision with 'poor' attention can be compared
		  to normal vision with 'full' attention to reveal how
		  attention alters visual perception. We report large
		  differences between residual and normal visual thresholds
		  for discriminating the orientation or spatial frequency of
		  simple patterns, and smaller differences for discriminating
		  contrast. A computational model, in which attention
		  activates a winner-take-all competition among overlapping
		  visual filters, quantitatively accounts for all
		  observations. Our model predicts that the effects of
		  attention on visual cortical neurons include increased
		  contrast gain as well as sharper tuning to orientation and
		  spatial frequency.}
}

