@Article{Kirkpatrick_etal17,
author		= {Kirkpatrick, James and Pascanu, Razvan and Rabinowitz,
		  Neil and Veness, Joel and Desjardins, Guillaume and Rusu,
		  Andrei A. and Milan, Kieran and Quan, John and Ramalho,
