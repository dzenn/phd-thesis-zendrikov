@InProceedings{Muir_Sitte03,
author		= {D. Muir and J. Sitte},
title		= {Seeing cheaply: flexible vision for small devices},
booktitle	= {2nd International Symposium on Autonomous Minirobots for
		  Research and Edutainment, ({AMiRE})},
year		= {2003},
month		= feb,
abstract	= {In this paper, we outline an architecture for supporting
		  real time autonomous vision in small devices. Our vision
		  processor performs data reduction at the point of image
		  capture, using re-configurable hardware devices to extract
		  and describe image features. By implementing our design on
		  FPGAs, the vision processor can support highly parallel
		  vision processing structures as well as more traditional
