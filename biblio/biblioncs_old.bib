@InProceedings{Khan_etal08,
author		= {Khan, M.M. and Lester, D.R. and Plana, L.A. and Rast, A.
		  and Jin, X. and Painkras, E. and Furber, S.B.},
title		= {{SpiNNaker}: {Mapping} neural networks onto a
